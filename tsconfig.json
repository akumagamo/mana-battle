{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "sourceMap": true,
        "module": "ES6",
        "target": "es5",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "allowJs": true,
        "strict": true,
        "strictNullChecks": true,
        "pretty": true
    }
}
